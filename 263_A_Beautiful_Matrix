mat = []
for i in range(5):
    mat.append([*map(int, input().split())])
    
pos = []

for i in range(5):
    for j in range(5):
        if mat[i][j] == 1:
            pos.append((i+1,j+1))
            break
            
            
    
# print(mat)
# print(pos)

print(abs(3-pos[0][0])+abs(3-pos[0][1]))
